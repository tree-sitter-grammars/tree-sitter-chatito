{
  "name": "tree-sitter-chatito",
  "version": "0.4.0",
  "license": "MIT",
  "description": "Chatito grammar for tree-sitter",
  "repository": "tree-sitter-grammars/tree-sitter-chatito",
  "author": {
    "name": "ObserverOfTime",
    "email": "chronobserver@disroot.org",
    "url": "https://observeroftime.github.io/"
  },
  "funding": {
    "url": "https://github.com/sponsors/ObserverOfTime",
    "type": "github"
  },
  "keywords": [
    "tree-sitter",
    "parser",
    "lexer",
    "chatito",
    "chatl"
  ],
  "scripts": {
    "prepare": "node -e \"require('./build')\"",
    "test": "tree-sitter test"
  },
  "main": "bindings/node",
  "dependencies": {
    "nan": "^2.18.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "~0.20.8"
  },
  "tree-sitter": [
    {
      "scope": "source.chatito",
      "file-types": [
        "chatito"
      ]
    },
    {
      "scope": "source.chatl",
      "path": "extensions/chatl",
      "file-types": [
        "chatl"
      ],
      "external-files": [
        "../../grammar.js"
      ]
    },
    {
      "scope": "source.chatette",
      "path": "extensions/chatette",
      "file-types": [
        "chatette"
      ],
      "external-files": [
        "../../grammar.js"
      ]
    }
  ]
}
